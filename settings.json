{
    "workbench.colorTheme": "Material Theme Palenight",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "editor.suggestSelection": "first",
    "editor.formatOnSave": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.requirements.JDK11Warning": true,
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-1.8",
            "javadoc": "https://docs.oracle.com/javase/8/docs/api",
            "path": "$JAVA_HOME8",
            "sources": "$JAVA_HOME8/lib/src.zip"
        },
        {
            "name": "JavaSE-11",
            "javadoc": "https://docs.oracle.com/en/java/javase/11/docs/api",
            "path": "$JAVA_HOME11",
            "sources": "$JAVA_HOME11/lib/src.zip"
        },
        {
            "default": true,
            "name": "JavaSE-15",
            "javadoc": "https://docs.oracle.com/en/java/javase/15/docs/api",
            "path": "$JAVA_HOME15",
            "sources": "$JAVA_HOME15/lib/src.zip"
        }
    ],
    "terminal.external.windowsExec": "pwsh",
    "terminal.external.osxExec": "iTerm2.app",
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.shell.windows": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
    "terminal.integrated.fontFamily": "Fira Code",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.automationShell.osx": "",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "terminal.integrated.env.osx": {
        "ZSH_THEME": "amuse"
    },
    "terminal.integrated.env.linux": {
        "ZSH_THEME": "amuse"
    },
    "java.refactor.renameFromFileExplorer": "autoApply",
    "java.implementationsCodeLens.enabled": true,
    "java.semanticHighlighting.enabled": true,
    "java.editor.defaultFormatter": "redhat.java",
    "java.completion.enabled": true,
}