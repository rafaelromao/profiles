{
    "workbench.colorTheme": "Material Theme Palenight",
    "java.semanticHighlighting.enabled": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "editor.fontSize": 18,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.requirements.JDK11Warning": false,
    "java.configuration.runtimes": [
           {
            "name": "JavaSE-1.8",
            "javadoc" : "https://docs.oracle.com/javase/8/docs/api",
            "path": "$JAVA_HOME8",
            "sources" : "$JAVA_HOME8/lib/src.zip"
           },
           {
            "name": "JavaSE-11",
            "javadoc" : "https://docs.oracle.com/en/java/javase/11/docs/api",
            "path": "$JAVA_HOME11",
            "sources" : "$JAVA_HOME11/lib/src.zip"
           },
           {
            "default":  true,
            "name": "JavaSE-14",
            "javadoc" : "https://docs.oracle.com/en/java/javase/14/docs/api",
            "path": "$JAVA_HOME14",
            "sources" : "$JAVA_HOME14/lib/src.zip"
           }
    ],
    "java.home": "$JAVA_HOME14",
    "terminal.external.windowsExec": "pwsh",
    "terminal.integrated.automationShell.osx": "",
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.shell.windows": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
    "terminal.integrated.fontFamily": "Noto Mono for Powerline",
    "terminal.integrated.fontSize": 16,
}